<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawer_layout"
    tools:context=".TratamientosActivity"
    tools:openDrawer="start">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginTop="80dp"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="14dp"
                android:text="@string/tratamiento"
                android:textSize="22sp"/>

            <!-- Contenedor 1 1-->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/rectangulo1"
                android:layout_width="wrap_content"
                android:layout_height="214dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="14dp"
                android:layout_marginTop="135dp"
                android:layout_marginEnd="215dp"
                android:background="@drawable/esquinas_redondeadas">
                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/imageView1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/rectangulo1"
                    app:riv_corner_radius="20dp"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/chronologis"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:text="Chronologiste"
                android:textSize="18sp"
                android:layout_marginTop="360dp"/>
            <TextView
                android:id="@+id/some_id"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginTop="385dp"
                android:textSize="16sp"
                android:text="$250.00"/>


            <!-- Contenedor 1 2 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/rectangulo2"
                android:layout_width="182dp"
                android:layout_height="215dp"
                android:layout_alignParentTop="true"
                android:layout_marginStart="216dp"
                android:layout_marginTop="135dp"
                android:background="@drawable/esquinas_redondeadas">
                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/imageView2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/rectangulo2"
                    app:riv_corner_radius="20dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/balayage"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="216dp"
                android:layout_marginTop="360dp"
                android:textSize="18sp"
                android:text="Balayage"/>
            <TextView
                android:id="@+id/some2_id"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="216dp"
                android:layout_marginTop="385dp"
                android:textSize="16sp"
                android:text="$60.00"/>


            <!-- Contenedor 2 1 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/rectangulo3"
                android:layout_width="182dp"
                android:layout_height="215dp"
                android:layout_alignParentTop="true"
                android:layout_marginStart="14dp"
                android:layout_marginTop="432dp"
                android:background="@drawable/esquinas_redondeadas">

                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/imageView3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/rectangulo3"
                    app:riv_corner_radius="20dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/blond_andol"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginTop="657dp"
                android:textSize="18sp"
                android:text="Bland Andolu"/>
            <TextView
                android:id="@+id/some3_id"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginTop="682dp"
                android:textSize="16sp"
                android:text="$55.00"/>


            <!-- Contenedor 2 2 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/rectangulo4"
                android:layout_width="182dp"
                android:layout_height="215dp"
                android:layout_marginStart="216dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="432dp"
                android:background="@drawable/esquinas_redondeadas">
                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/imageView4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/rectangulo4"
                    app:riv_corner_radius="20dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/discipline"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="217dp"
                android:layout_marginTop="657dp"
                android:textSize="18sp"
                android:text="Discipline"/>
            <TextView
                android:id="@+id/some4_id"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="216dp"
                android:layout_marginTop="682dp"
                android:textSize="16sp"
                android:text="$40.00"/>


            <!-- Contenedor 3 1 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/rectangulo5"
                android:layout_width="182dp"
                android:layout_height="215dp"
                android:layout_marginStart="14dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="729dp"
                android:background="@drawable/esquinas_redondeadas">
                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/imageView5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/rectangulo5"
                    app:riv_corner_radius="20dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/genesis"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginTop="955dp"
                android:textSize="18sp"
                android:text="Genesis"/>
            <TextView
                android:id="@+id/some5_id"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginTop="980dp"
                android:layout_marginBottom="35dp"
                android:textSize="16sp"
                android:text="$55.00"/>


            <!-- Contenedor 3 2 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/rectangulo6"
                android:layout_width="182dp"
                android:layout_height="215dp"
                android:layout_marginStart="216dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="729dp"
                android:background="@drawable/esquinas_redondeadas">

                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/imageView6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/rectangulo6"
                    app:riv_corner_radius="20dp"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:id="@+id/k_ratase"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="216dp"
                android:layout_marginTop="955dp"
                android:textSize="18sp"
                android:text="Kératase"/>
            <TextView
                android:id="@+id/some6_id"
                android:layout_width="182dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="216dp"
                android:layout_marginTop="980dp"
                android:layout_marginBottom="35dp"
                android:textSize="16sp"
                android:text="$65.00"/>

        </RelativeLayout>

    </ScrollView>

    <include
        layout="@layout/app_bar_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:headerLayout="@layout/nav_header_main"
        app:menu="@menu/activity_main_drawer" >
    </com.google.android.material.navigation.NavigationView>

</androidx.drawerlayout.widget.DrawerLayout>